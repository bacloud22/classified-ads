{
  "name": "classified-ads",
  "version": "1.0.0",
  "description": "A performant classified-ads website with maps and tags",
  "main": "index.js",
  "scripts": {
    "build": "sudo apt-get install -y build-essential && npm install && cd client && npm install && npm run dev:client && cd ..",
    "start": "node app.js",
    "dev:start": "~/go/bin/MailHog & node app.js",
    "stop": "pkill --signal SIGINT classified-ads && kill -9 `pidof ~/go/bin/MailHog`",
    "tools": "sudo apt-get -y install golang-go && go get github.com/mailhog/MailHog && cd tools && curl -L -O https://github.com/jcbvm/i18n-editor/releases/download/1.0.0/i18n-editor-1.0.0.zip --output i18n-editor-1.0.0.zip && unzip i18n-editor-1.0.0.zip"
  },
  "keywords": [
    "Node.js",
    "JavaScript",
    "MongoDB",
    "Maps",
    "web-app"
  ],
  "author": "bacloud22",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^6.1.1",
    "@google-cloud/storage": "^5.18.3",
