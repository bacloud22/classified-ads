<% if(typeof data.section !='undefined' && typeof data.parent !='undefined' && typeof data.granpa !='undefined' ){ %>

<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="#"><%- data.parent %></a></li>
    <li class="breadcrumb-item"><a href="#"><%- data.granpa %></a></li>
    <li class="breadcrumb-item active" aria-current="page"><%- data.title.charAt(0).toUpperCase() + data.title.slice(1) %></li>
  </ol>
</nav>

<% } %>